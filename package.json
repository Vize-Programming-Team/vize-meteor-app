{
  "name": "vize-site",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "type-check": "flow",
    "lint": "eslint -c .eslintrc.json ./{client,server,imports}/**/*.{js,jsx}",
    "lint-nice-out": "eslint -c .eslintrc.json ./{client,server,imports}/**/*.{js,jsx} -f html -o ./eslint-out.html",
    "lint-formatting": "prettier --list-different ./**/*.{js,jsx,css,scss}"
  },
  "cssModules": {
    "extensions": [
      "css",
      "mss",
      "m.css",
      "scss"
    ],
    "passthroughPaths": [
      "^imports/sass/.*\\.scss",
      "^client/main\\.scss"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.47",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "apollo-boost": "^0.1.7",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-server-express": "^1.3.6",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "lint": "^1.1.2",
    "lodash.find": "^4.6.0",
    "message-box": "^0.2.0",
    "meteor-node-stubs": "^0.2.11",
    "pg": "^7.4.3",
    "pg-live-select": "^1.0.3",
    "pg-native": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-apollo": "^2.1.4",
    "react-circular-progressbar": "^0.8.0",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-simple-dropdown": "^3.2.0",
    "react-star-rating-component": "^1.3.0",
    "react-star-ratings": "^2.2.0",
    "simpl-schema": "^1.4.3"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^8.2.3",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-meteor": "^5.0.0",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.74.0",
    "node-sass": "^4.9.3",
    "prettier": "1.12.1",
    "react-test-renderer": "^16.4.2"
  }
}
